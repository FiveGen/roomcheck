<%= form_for (@check_list) do |f| %>
	<% @check_list.errors.full_messages.each do |msg| %>
		<%= msg %>
	<% end %>
	<%= f.label :name, "Room name or number" %>
	<%= f.text_field :name %>
<br/>
	<%= f.submit("Save", :class => "btn btn-primary") %> <%= link_to "Cancel", check_lists_path, :class => "btn" %>
<% end %>